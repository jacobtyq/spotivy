{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Launch ts-node",
    "runtimeArgs": [
      "-r",
      "ts-node/register"
    ],
    "args": ["${workspaceFolder}/src/index.ts", "init", "--verbose"],
    // "args": ["playlist", "danguilherme", "-v", "--flat"],
    // "args": ["playlist", "danguilherme", "0E2o1alCwoZnJzWuCgGuVa", "--verbose"],
    // "args": ["track", "0SFJQqnU0k42NYaLb3qqTx", "31acMiV67UgKn1ScFChFxo", "--verbose"],
    "cwd": "${workspaceRoot}",
    "env": {
      "NODE_ENV": "development"
    },
    "console": "integratedTerminal",
  }]
}